<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="PrintGaransi.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<connectionStrings>
		<!-- Connection string with proper TrustServerCertificate -->
		<add name="LSBUConnection" connectionString="Server=.\SQLEXPRESS;Database=LSBU;Integrated Security=True;TrustServerCertificate=True;" />
	</connectionStrings>
	<userSettings>
		<PrintGaransi.Properties.Settings>
			<setting name="ServerIP" serializeAs="String">
				<value>127.0.0.1</value>
				<!-- Nilai default -->
			</setting>
			<setting name="Port" serializeAs="String">
				<value>1433</value>
				<!-- Port default SQL Server -->
			</setting>
			<setting name="Location" serializeAs="String">
				<value>Main Office</value>
				<!-- Contoh lokasi default -->
			</setting>
			<setting name="LocationID" serializeAs="String">
				<value>1</value>
				<!-- Contoh ID lokasi default -->
			</setting>
			<setting name="ProductType" serializeAs="String">
				<value>Printer</value>
				<!-- Nilai default -->
			</setting>
			<setting name="ScanTime" serializeAs="String">
				<value>10</value>
				<!-- Waktu default -->
			</setting>
			<setting name="Mode" serializeAs="String">
				<value>Auto</value>
				<!-- Nilai default -->
			</setting>
			<setting name="PrinterType" serializeAs="String">
				<value>Laser</value>
				<!-- Contoh printer default -->
			</setting>
			<setting name="CheckBoxChecked" serializeAs="String">
				<value>False</value>
			</setting>
		</PrintGaransi.Properties.Settings>
	</userSettings>
</configuration>
